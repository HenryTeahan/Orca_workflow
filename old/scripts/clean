rm -f *.bibtex
rm -f *.cpcm
rm -f *.cpcm_corr
rm -f *.densities
rm -f *.densitiesinfo
rm -f *.engrad
rm -f *.gbw
rm -f *.hess
rm -f *.property.txt
mkdir -p INP_files
mkdir -p Trajectories
